{
  "name": "craco-less",
  "version": "1.17.1",
  "description": "A Less plugin for craco / react-scripts / create-react-app",
  "main": "lib/craco-less.js",
  "scripts": {
    "test": "jest",
    "test:debug": "node --inspect-brk node_modules/.bin/jest --runInBand",
    "lint": "eslint --fix lib",
    "format": "prettier --write **/*.js",
    "update_deps": "node scripts/update_deps"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/DocSpring/craco-less.git"
  },
  "keywords": [
    "craco",
    "create-react-app"
  ],
  "author": "Nathan Broadbent",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/DocSpring/craco-less/issues"
  },
  "homepage": "https://github.com/DocSpring/craco-less#readme",
  "devDependencies": {
    "@craco/craco": "^6.1.2",
    "coveralls": "^3.0.2",
    "eslint": "^7.28.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.0.0",
    "eslint-plugin-promise": "^5.1.0",
    "husky": "^6.0.0",
    "jest": "^27.0.4",
    "jest-extended": "^0.11.5",
    "lint-staged": "^11.0.0",
    "lodash": "^4.17.21",
    "prettier": "^2.3.0",
    "react-scripts": "^4.0.3",
    "webpack": "4.44.2"
  },
  "dependencies": {
    "less": "^4.1.1",
    "less-loader": "^8.1.1"
  },
  "peerDependencies": {
    "@craco/craco": "~5 || ~6",
    "react-scripts": "~3 || ~4"
  },
  "resolutions": {
    "react-scripts/webpack-dev-server": ">=3.11.0"
  }
}
